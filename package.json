  "description": "Mullet Stack: Facebook in the front. Walmart in the back. (React.js/Hapi)",
  "main": "walmart.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "engines": {
    "node": "^0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lynnaloo/mullet.git"
  },
  "keywords": [
    "react",
    "hapi",
    "node",
    "Facebook",
    "Walmart"
  ],
  "author": "Linda Nichols",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lynnaloo/mullet/issues"
  },
  "homepage": "http://www.mullet.io",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-core": "^6.4.0",
    "babel-jest": "^5.3.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "blipp": "^2.3.0",
    "body-parser": "^1.14.1",
    "bootstrap": "^3.3.5",
    "h2o2": "^4.0.2",
    "hapi": "^9.3.1",
    "immutable": "^3.7.6",
    "inert": "^3.2.0",
    "jquery": "^2.1.4",
    "npm-watch": "^0.0.1",
    "react": "^0.13.3",
    "redux": "^3.3.1",
    "vision": "^3.0.0",
    "webpack": "^1.12.10"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "code": "^1.5.0",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "jest-cli": "^0.4.19",
    "lab": "^5.15.2",
    "mocha": "^2.4.5"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
